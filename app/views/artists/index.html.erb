<h1>Artists</h1>
<tr>
  <th>Name:</th>
</tr>

<table class="table table-striped">

  <% @artists.each do |artist| %>
    <tr>
      <td><%= artist.name %></td>
      <td><%= link_to 'Show', artist_path(artist) %></td>
      <td><%= link_to 'Destroy', artist_path(artist),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
