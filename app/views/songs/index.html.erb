<h1>Songs</h1>
<tr>
  <th>Title:</th>
</tr>

<table class="table table-striped">

  <% @songs.each do |song| %>
    <tr>
      <td><%= song.title %></td>
      <td><%= link_to 'Show', song_path(song) %></td>
      <td><%= link_to 'Destroy', song_path(song),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>
